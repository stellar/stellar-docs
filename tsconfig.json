{
  "compilerOptions": {
    "target": "ES6",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "jsx": "react",
    "noEmit": true,
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "include": [
    "src/**/*",
    "openrpc/**/*",
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
  "extends": [
    "@docusaurus/tsconfig",
    "@stellar/tsconfig"
  ]
}
