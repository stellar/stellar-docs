{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "noEmit": true,
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "include": [
    "src/**/*",
    "openrpc/**/*",
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
  "extends": [
    "@docusaurus/tsconfig"
  ]
}
